{
  "appFolders": [
    "app"
  ],
  "testFolders": [
    "test"
  ],
  "enableExternalRulesets": true,
  "enableCodeCop": true,
  "enableUICop": true,
  "enableAppSourceCop": true,
  "enablePerTenantExtensionCop": true,
  "removeInternalsVisibleTo": false,
  "overrideResourceExposurePolicy": false,
  "failPublishTestsOnFailureToPublishResults": false,
  "cacheImageName": "",
  "writableFolderPath": "C:\\_BCFusion5Apps",
  "versioningStrategy": 2,
  "versioningTimeOffset": 10,
  "artifactUrlCacheKeepHours": 12,
  "pipelineFolderStructure": "Repository",
  "pipelineBranch": "test",
  "pipelineSkipFirstRun": true,
  "preprocessorSymbols": [
    "CLEAN25"
  ],
  "BCDevOpsFlowsPoolName": "BCDevOpsBuilds",
  "BCDevOpsFlowsPoolNameCICD": "BCDevOpsBuildsCICD",
  "BCDevOpsFlowsPoolNamePublishToProd": "BCDevOpsBuildsPublishToProd",
  "BCDevOpsFlowsResourceRepositoryName": "TKapitan/BCDevOpsFlows",
  "BCDevOpsFlowsServiceConnectionName": "BCDevOpsFlowsServiceConnection",
  "BCDevOpsFlowsResourceRepositoryBranch": "preview",
  "BCDevOpsFlowsVariableGroup": "BCDevOpsFlows",
  "failOn": "Error",
  "runWith": "BCContainerHelper",
  "recreatePipelineInSetupPipeline": true,
  "allowPrerelease": true,
  "ConditionalSettings": [
    {
      "branches": [
        "main",
        "master"
      ],
      "settings": {
        "allowPrerelease": false
      }
    },
    {
      "workflows": [
        "CICD"
      ],
      "settings": {
        "artifact": "////appjson",
        "customCodeCops": [],
        "removeInternalsVisibleTo": true,
        "overrideResourceExposurePolicy": true,
        "allowDebugging": true,
        "allowDownloadingSource": false,
        "includeSourceInSymbolFile": false,
        "applyToDevExtension": true,
        "enableCodeCop": false,
        "enableUICop": false,
        "enableAppSourceCop": false,
        "enablePerTenantExtensionCop": false,
        "runWith": "NuGet",
        "preprocessorSymbols": [
          "PRERELEASE",
          "PREVIEW",
          "TEST"
        ],
        "workflowTrigger": {
          "batch": true,
          "branches": {
            "include": [
              "test",
              "preview"
            ]
          },
          "paths": {
            "exclude": [
              ".azure-pipelines"
            ]
          }
        }
      }
    },
    {
      "workflows": [
        "PublishToProduction"
      ],
      "settings": {
        "artifact": "////appjson",
        "customCodeCops": [],
        "removeInternalsVisibleTo": true,
        "overrideResourceExposurePolicy": true,
        "allowDebugging": true,
        "allowDownloadingSource": false,
        "includeSourceInSymbolFile": false,
        "applyToDevExtension": true,
        "enableCodeCop": false,
        "enableUICop": false,
        "enableAppSourceCop": false,
        "enablePerTenantExtensionCop": false,
        "updateVersionNumber": "+0.1",
        "runWith": "NuGet",
        "ignoredPreprocessorSymbols": [
          "PRERELEASE",
          "PREVIEW",
          "TEST"
        ],
        "workflowTrigger": {
          "batch": true,
          "branches": {
            "include": [
              "main",
              "master"
            ]
          },
          "paths": {
            "exclude": [
              ".azure-pipelines"
            ]
          }
        }
      }
    },
    {
      "workflows": [
        "PullRequest"
      ],
      "settings": {
        "artifact": "////appjson",
        "removeInternalsVisibleTo": false,
        "failOn": "Warning",
        "preprocessorSymbols": [
          "PRERELEASE",
          "PREVIEW",
          "TEST"
        ],
        "customCodeCops": [],
        "enableLinterCop": true
      }
    },
    {
      "workflows": [
        "TestCurrent"
      ],
      "settings": {
        "artifact": "////latest",
        "skipUpgrade": true,
        "ignoredPreprocessorSymbols": [
          "PRERELEASE",
          "PREVIEW",
          "TEST"
        ],
        "workflowSchedule": {
          "cron": "0 15 * * 3",
          "displayName": "Every Thursday at 1 am (Brisbane time)",
          "always": true,
          "batch": true,
          "branches": {
            "include": [
              "test"
            ]
          }
        }
      }
    },
    {
      "workflows": [
        "TestNextMajor"
      ],
      "settings": {
        "artifact": "////nextmajor",
        "skipUpgrade": true,
        "ignoredPreprocessorSymbols": [
          "PRERELEASE",
          "PREVIEW",
          "TEST"
        ],
        "preprocessorSymbols": [
          "CLEAN26",
          "CLEAN27"
        ],
        "workflowSchedule": {
          "cron": "0 15 14 * *",
          "displayName": "15th of every month at 1 am (Brisbane time)",
          "always": true,
          "batch": true,
          "branches": {
            "include": [
              "test"
            ]
          }
        }
      }
    },
    {
      "workflows": [
        "TestNextMinor"
      ],
      "settings": {
        "artifact": "////nextminor",
        "skipUpgrade": true,
        "ignoredPreprocessorSymbols": [
          "PRERELEASE",
          "PREVIEW",
          "TEST"
        ],
        "preprocessorSymbols": [
          "CLEAN26"
        ],
        "workflowSchedule": {
          "cron": "0 15 24 * *",
          "displayName": "25th of every month at 1 am (Brisbane time)",
          "always": true,
          "batch": true,
          "branches": {
            "include": [
              "test"
            ]
          }
        }
      }
    }
  ]
}